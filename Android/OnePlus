#!/bin/sh
adb shell pm uninstall --user 0 cn.oneplus.photos # Shot On OnePlus
adb shell pm uninstall --user 0 com.example.wifirftest # Wifi Radio Frequency test
adb shell pm uninstall --user 0 com.fingerprints.fingerprintsensortest # Sensor Test Tool 
adb shell pm uninstall --user 0 com.oem.autotest # Auto Test Server
adb shell pm uninstall --user 0 com.oem.logkitsdservice
adb shell pm uninstall --user 0 com.oem.nfc #OnePlus NFC tester
adb shell pm uninstall --user 0 com.oem.oemlogkit # OnePlusLogKit  # See adb shell pm uninstall --user 0 com.oem.logkitsdservice
adb shell pm uninstall --user 0 com.oneplus.backuprestore # OnePlus Switch (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.oneplus.backuprestore)
adb shell pm uninstall --user 0 com.oneplus.brickmode # OnePlus Zen Mode (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.oneplus.brickmode)
adb shell pm uninstall --user 0 com.oneplus.bttestmode # OnePlus Bluetooth test mode
adb shell pm uninstall --user 0 com.oneplus.card # Card Package
adb shell pm uninstall --user 0 com.oneplus.factorymode # EngineeringMode/ FactoryMode
adb shell pm uninstall --user 0 com.oneplus.factorymode.specialtest # Engineering Mode Special Test
adb shell pm uninstall --user 0 com.oneplus.gamespace # OnePlus Game Space
adb shell pm uninstall --user 0 com.oneplus.note # OnePlus Notes app
adb shell pm uninstall --user 0 com.oneplus.opbugreportlite # OPBugReportLite 
adb shell pm uninstall --user 0 com.oneplus.soundrecorder # OnePlus voice recording app 
adb shell pm uninstall --user 0 com.tencent.soter.soterserver # Soter is a biometric authentication standard and platform in Android held by Tencent.
adb shell pm uninstall --user 0 com.wapi.wapicertmanage # WAPI certificate manager
net.oneplus.commonlogtool # OnePlus Common Log Tool
net.oneplus.forums # OnePlus Community (https://play.google.com/store/apps/details?id=net.oneplus.forums)
adb shell pm uninstall --user 0 com.oneplus.opsports # Cricket Scores (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.oneplus.opsports)
net.oneplus.odm
net.oneplus.odm.provider # Shady analytic app... again.
net.oneplus.provider.appcategoryprovider # AppCategoryProvider
net.oneplus.push #OnePlus push notification. 
net.oneplus.weather # OnePlus weather app (https://play.google.com/store/apps/details?id=net.oneplus.weather)
net.oneplus.widget # OnePlus Widget
#adb shell pm uninstall --user 0 com.oneplus.iconpack.circle # OnePlus Icon Pack - Round (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.oneplus.iconpack.circle)
#adb shell pm uninstall --user 0 com.oneplus.iconpack.oneplus # OnePlus Icon Pack (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.oneplus.iconpack.oneplus)
#adb shell pm uninstall --user 0 com.oneplus.iconpack.square # OnePlus Icon Pack - Square (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.oneplus.iconpack.square)
#cn.oneplus.oem_tcma # TCMA stands for Tiered Contention Multiple Access, which is a cellular traffic management protocol.
#adb shell pm uninstall --user 0 com.oneplus.filemanager # OnePlus file manager
