#!/bin/sh
adb shell pm uninstall --user 0 android.autoinstalls.config.motorola.layout # Device configuration for Motorola 
adb shell pm uninstall --user 0 com.lenovo.lsf.user # Lenovo ID adds an option in Settings>Accounts where you can login to a Lenovo ID account.
adb shell pm uninstall --user 0 com.lmi.motorola.rescuesecurity # Rescue Security by LogMeIn (https://www.logmeinrescue.com/)
adb shell pm uninstall --user 0 com.motorola.android.fmradio #FMRadioService 
adb shell pm uninstall --user 0 com.motorola.android.jvtcmd # Java Tcmd Helper
adb shell pm uninstall --user 0 com.motorola.android.nativedropboxagent # Native DropBox Agent.
adb shell pm uninstall --user 0 com.motorola.android.providers.chromehomepage # Seems to only prodive the home symbol in Chrome.
adb shell pm uninstall --user 0 com.motorola.android.provisioning # OMA Client Provisioning
adb shell pm uninstall --user 0 com.motorola.android.settings.diag_mdlog # diag_mdlog is a small proprietary Qualcomm program which can store DIAG logs on the filesystem. ( discontinued )
adb shell pm uninstall --user 0 com.motorola.android.settings.modemdebug # Provide modem debug settings menu ? ( discontinued )
adb shell pm uninstall --user 0 com.motorola.appdirectedsmsproxy # An Application directed SMS (or rather a Port directed SMS) is an SMS directed to a specific port. 
adb shell pm uninstall --user 0 com.motorola.bach.modemstats # ModemStatsService
adb shell pm uninstall --user 0 com.motorola.brapps # Motorola App Box (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.motorola.brapps)
adb shell pm uninstall --user 0 com.motorola.bug2go # Bugs reporting app that sends info about crash report.
adb shell pm uninstall --user 0 com.motorola.ccc.devicemanagement # Mobile Device Management (MDM) allows companyâ€™s IT department to reach inside your phone in the background
adb shell pm uninstall --user 0 com.motorola.ccc.mainplm # plm = Product Lifecycle Management ?  # ccc = China Compulsory Certification
adb shell pm uninstall --user 0 com.motorola.ccc.notification # Motorola Notifications (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.motorola.ccc.notification)
adb shell pm uninstall --user 0 com.motorola.contacts.preloadcontacts # Preloaded Contacts
adb shell pm uninstall --user 0 com.motorola.demo # Moto Demo Mode 
adb shell pm uninstall --user 0 com.motorola.demo.env # Needed for Moto Demo Mode
adb shell pm uninstall --user 0 com.motorola.easyprefix # Motorola Easy Prefix (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.motorola.easyprefix)
adb shell pm uninstall --user 0 com.motorola.email # Moto Email (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.motorola.email)
adb shell pm uninstall --user 0 com.motorola.fmplayer # FM Radio (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.motorola.fmplayer)
adb shell pm uninstall --user 0 com.motorola.frameworks.singlehand # Provide the Single/One hand mode
adb shell pm uninstall --user 0 com.motorola.genie # Device Help (previously Moto Help) (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.motorola.genie)
adb shell pm uninstall --user 0 com.motorola.gesture # Gesture navigation tutorial added in Android 10.
adb shell pm uninstall --user 0 com.motorola.help # Moto feedback (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.motorola.help)
adb shell pm uninstall --user 0 com.motorola.lifetimedata # Not 100% sure but it's most likely the Total Call Timer or more generally it handles info like the date of manufacture of your device,
adb shell pm uninstall --user 0 com.motorola.hiddenmenuapp # Added in Android 10. Safe to remove.
adb shell pm uninstall --user 0 com.motorola.moto # Moto (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.motorola.moto)
adb shell pm uninstall --user 0 com.motorola.motocare # Moto Care was renamed in Moto Help and then in Device Help
adb shell pm uninstall --user 0 com.motorola.motocare.internal # Core stuff for the package above I guess.
adb shell pm uninstall --user 0 com.motorola.motocit # CQATest
adb shell pm uninstall --user 0 com.motorola.motodisplay # Moto Display (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.motorola.motodisplay)
adb shell pm uninstall --user 0 com.motorola.paks # ADB: Package Protected.
adb shell pm uninstall --user 0 com.motorola.programmenu # Programming Menu
adb shell pm uninstall --user 0 com.motorola.ptt.prip # Prip (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.motorola.ptt.prip)
adb shell pm uninstall --user 0 com.motorola.setup # Related to Motorola Account setup (only during first boot ?)
adb shell pm uninstall --user 0 com.motorola.slpc_sys # Motorola Slpc System
adb shell pm uninstall --user 0 com.motorola.timeweatherwidget # Provides time/weather widget on the home screen.
#adb shell pm uninstall --user 0 com.motorola.camera2 # Moto Camera 2 (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.motorola.camera)
adb shell pm uninstall --user 0 com.motorola.actions # Moto Actions (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.motorola.actions)
adb shell pm uninstall --user 0 com.motorola.carriersettingsext # Carrier settings contains APN settings for instance.
#adb shell pm uninstall --user 0 com.motorola.callredirectionservice # Added in Android 10. Safe to remove.
#adb shell pm uninstall --user 0 com.motorola.ccc.ota # Motorola Update Services
adb shell pm uninstall --user 0 com.motorola.comcast.settings.extensions # Most likely provides a special settings menu for Comcast stuff.
adb shell pm uninstall --user 0 com.motorola.comcastext # See above. Provide special (useless) features from Comcast? App title is Activation.
#adb shell pm uninstall --user 0 com.motorola.visualvoicemail # Verizon Visual Voicemail (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.motorola.visualvoicemail)
#adb shell pm uninstall --user 0 com.motorola.entitlement # Enable WiFi tethering/hotspot functionality. 
adb shell pm uninstall --user 0 com.motorola.faceunlock # Moto Face Unlock (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.motorola.faceunlock)
adb shell pm uninstall --user 0 com.motorola.faceunlocktrustagent # Motorola Face Unlock Agent
#adb shell pm uninstall --user 0 com.motorola.imagertuning_athene
#adb shell pm uninstall --user 0 com.motorola.imagertuning_ocean
#adb shell pm uninstall --user 0 com.motorola.imagertuning_lake # Imager Tuning (https://play.google.com/store/apps/details?id=adb shell pm uninstall --user 0 com.motorola.imagertuning_athene)
adb shell pm uninstall --user 0 com.motorola.invisiblenet # Invisible net
#adb shell pm uninstall --user 0 com.motorola.launcher3 # Motorola system Launcher
