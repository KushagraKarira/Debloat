#!/bin/sh
adb shell pm uninstall --user 0 com.android.keyguard #Huwei Magazine Unlock
adb shell pm uninstall --user 0 com.android.hwmirror #Mirror
adb shell pm uninstall --user 0 com.baidu.input_huawei #Keyboard ( and spyware )
adb shell pm uninstall --user 0 com.hicloud.android.clone #Hawvei Phone Clone
adb shell pm uninstall --user 0 com.huawei.android.chr #Huawei Call History Record 
adb shell pm uninstall --user 0 com.huawei.android.FloatTasks #Floating dock function 
adb shell pm uninstall --user 0 com.huawei.android.hsf # Huawei Services Framework
adb shell pm uninstall --user 0 com.huawei.android.hwpay #Huawei Pay
adb shell pm uninstall --user 0 com.huawei.android.instantonline # no noticable consequences
adb shell pm uninstall --user 0 com.huawei.android.instantshare  # Huawei Share features.
adb shell pm uninstall --user 0 com.huawei.android.karaoke # Karaoke mode feature.
adb shell pm uninstall --user 0 com.huawei.android.mirrorshare  # MirrorShare feature (Miracast rebranded by Huawei)
adb shell pm uninstall --user 0 com.huawei.android.pushagent # push notification agent for Huawei apps
adb shell pm uninstall --user 0 com.huawei.android.remotecontroller # Huawei Smart Controller app.
adb shell pm uninstall --user 0 com.huawei.android.tips  # HUAWEI Feature Advisor
adb shell pm uninstall --user 0 com.huawei.android.totemweather 
adb shell pm uninstall --user 0 com.huawei.android.totemweatherapp
adb shell pm uninstall --user 0 com.huawei.android.totemweatherwidget # Huawei Weather app (and its widget)
adb shell pm uninstall --user 0 com.huawei.android.wfdft
adb shell pm uninstall --user 0 com.huawei.android.wfdirect  # Wi-Fi Direct feature.
adb shell pm uninstall --user 0 com.huawei.appmarket  # Huawei app store (AppGallery) # https://www.xda-developers.com/appgallery-huawei-alternative-google-play-store-android/
adb shell pm uninstall --user 0 com.huawei.arengine.service  # Augmented reality service.
adb shell pm uninstall --user 0 com.huawei.bd # HwUE (Huawei UserExperience)
adb shell pm uninstall --user 0 com.huawei.bluetooth # Lets you import your contacts via Bluetooth
adb shell pm uninstall --user 0 com.huawei.browser  # Huawei Browser app.
adb shell pm uninstall --user 0 com.huawei.browserhomepage # Huawei Browser component.
adb shell pm uninstall --user 0 com.huawei.compass # Huawei Compass app.
