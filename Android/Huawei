adb shell pm uninstall --user 0 com.huawei.hwvoipservice # MeeTime (https://consumer.huawei.com/en/support/content/en-us00956296/)
adb shell pm uninstall --user 0 com.huawei.iaware # App Prioritizer. Prioritizes apps to avoid slowdown
adb shell pm uninstall --user 0 com.huawei.ihealth  # MotionService package, it's required for actions like shaking the phone to shut off the alarm, ecc.
adb shell pm uninstall --user 0 com.huawei.intelligent # Huawei Assistant. Shopping recommendations
adb shell pm uninstall --user 0 com.huawei.health # Huawei Health (https://play.google.com/store/apps/details?id=adbcom.huawei.health)
