adb shell pm uninstall --user 0 com.huawei.contacts.sync # Huawei Contacts sync
adb shell pm uninstall --user 0 com.huawei.desktop.explorer # From XDA thread : Service that is been used when you wanna use your phone as an operative system on a PC.
adb shell pm uninstall --user 0 com.huawei.email # Huawei Email app.
adb shell pm uninstall --user 0 com.huawei.fido.uafclient  # UAF client for FIDO.
adb shell pm uninstall --user 0 com.huawei.game.kitserver # Probably safe to remove if you don't play games
adb shell pm uninstall --user 0 com.huawei.gameassistant  # Huawei Game Suite (HiGame).
adb shell pm uninstall --user 0 com.huawei.geofence  # GeofenceService.
adb shell pm uninstall --user 0 com.huawei.hwsearch # Petal Search widget. Used for finding apps/apks on serveral online sources (introduced after Google Mobile Services Ban).
adb shell pm uninstall --user 0 com.huawei.hwid  # Huawei Mobile Services (https://play.google.com/store/apps/details?id=adbcom.huawei.hwid)
adb shell pm uninstall --user 0 com.huawei.hiaction # no noticable consequences
adb shell pm uninstall --user 0 com.huawei.hiai # no noticable consequences
adb shell pm uninstall --user 0 com.huawei.hiassistantoversea # HiVoice. Huawei's voice assistant to replace Hey Google
adb shell pm uninstall --user 0 com.huawei.hicard # Huawei Cards, no noticable consequences
adb shell pm uninstall --user 0 com.huawei.hicloud # Huawei cloud features
adb shell pm uninstall --user 0 com.huawei.hifolder  # Huawei Online Cloud folder service
